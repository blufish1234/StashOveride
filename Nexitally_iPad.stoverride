name: "Stash Override for Nexitally"
desc: "提供自定義dns伺服器服務"

dns:
  nameserver: #!replace
    - https://dns.alidns.com/dns-query
  fallback: #!replace
    - https://dns.nextdns.io/b1bc9c/iPad
  default-nameserver: #!replace
    - 223.5.5.5
    - 119.29.29.29
  proxy-server-nameserver: #!replace
    - 119.29.29.29
    - 223.5.5.5
  follow-rule: True
  nameserver-policy: #!replace
    '*.xsatmo.lol': 223.5.5.5
    '*.ivdgoq.lol': 223.5.5.5
    '*.ckbuts.lol': 223.5.5.5
    'dns.nextdns.io': 119.29.29.29

rules: 
  - DOMAIN,dns.nextdns.io,PROXY
