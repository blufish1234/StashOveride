name: "Stash Override for Nexitally"
desc: "提供自定義dns伺服器服務"

dns:
  nameserver: #!replace
    - https://dns.alidns.com/dns-query
  fallback: #!replace
    - https://1.1.1.1/dns-query
  default-nameserver: #!replace
    - 1.1.1.1
    - 223.5.5.5
    - 119.29.29.29
  proxy-server-nameserver: #!replace
    - 119.29.29.29
    - 223.5.5.5
    - 1.1.1.1
  follow-rule: True
  nameserver-policy: #!replace
    "*.xsatmo.lol": 119.29.29.29
    "*.ivdgoq.lol": 119.29.29.29
    "*.ckbuts.lol": 119.29.29.29
