name: "Stash Override for Nexitally"
desc: "提供自定義dns伺服器服務"

dns:
  nameserver: #!replace
    - https://dns.nextdns.io/b1bc9c/iPad_Pro
  default-nameserver: #!replace
    - 1.1.1.1
    - 1.0.0.1
    - 223.5.5.5
    - 223.6.6.6
  respect-rules: True #!replace

rules:
